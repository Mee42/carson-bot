CarsonBot
    -global
    \- user
      -id

    --guilds
     \-guild
       -_id : id
       -leave_message
       -leave_channel

    --users
     \-user
       - _id : id (user + guild id)
       - user_id
       - guild_id
       - xp

    --gg
     \-user
       - _id : user_id
       - money
       - edu_level
       - debt
       - interest
       - coins
       - invested
       - gotten
     \-bank
       - id  :  bank
       - money

   --permissions
     \-entry
       -  _id : random
      o-  user_id
      o-  role_id
       -  guild_id
      o-  channel_id
       -  scope  : <GLOBAL/GUILD/CHANNEL>
       -  permission : <BOT_ADMIN/MOD/USER>